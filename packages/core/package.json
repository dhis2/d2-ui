{
  "name": "@dhis2/d2-ui-core",
  "description": "Core components for DHIS2 in d2-ui",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "license": "BSD-3-Clause",
  "author": "",
  "scripts": {
    "coverage": "jest --coverage",
    "lint": "eslint src/",
    "prebuild": "npm run lint && rimraf ./build/* && mkdirp ./build",
    "build": "npm run build:sass && npm run build:es",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir build --ignore spec.js",
    "build:sass": "node-sass ./scss --output ./build/css",
    "watch": "npm run build -- --watch",
    "test-ci": "jest --config=../../jest.config.js packages/core"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhis2/d2-ui.git"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "d2": "~31.1",
    "lodash": "^4.17.10",
    "material-ui": "^0.20.0"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0-PLACEHOLDER",
  "devDependencies": {
    "mkdirp": "^0.5.1"
  }
}
