{
    "name": "@dhis2/d2-ui-org-unit-dialog",
    "version": "0.1.0",
    "description": "Org Unit Dialog component for d2-ui",
    "main": "./build/cjs/index.js",
    "module": "./build/es/index.js",
    "files": [
        "build"
    ],
    "license": "BSD-3-Clause",
    "peerDependencies": {
        "d2": "^29.1.1",
        "react": "^16.0.0",
        "react-dom": "^16.0.0"
    },
    "author": "Ilya Nee <ilya@dhis2.org>",
    "scripts": {
        "prebuild": "rimraf ./build/*",
        "build": "npm run localize && npm run build:commonjs && npm run build:es",
        "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir build/cjs --ignore spec.js",
        "build:es": "cross-env BABEL_ENV=es babel src --out-dir build/es --ignore spec.js",
        "build:umd": "cross-env && webpack -p",
        "watch": "npm run build:es --  --watch",
        "test-ci": "jest --config=../../jest.config.js packages/org-unit-dialog",
        "extract-pot": "d2-i18n-extract -p src/ -o i18n/",
        "prettify": "prettier \"src/**/*.{js,jsx,json,css}\" --write",
        "localize": "yarn extract-pot && d2-i18n-generate -n NAMESPACE -p ./i18n/ -o ./src/locales/"
    },
    "dependencies": {
        "@dhis2/d2-i18n": "^1.0.3",
        "@dhis2/d2-ui-core": "^1.3.2",
        "@dhis2/d2-ui-org-unit-tree": "^1.0.13",
        "@material-ui/core": "^1.5.1",
        "@material-ui/icons": "^2.0.3",
        "prop-types": "^15.5.10"
    },
    "publishConfig": {
        "access": "public"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run extract-pot && git add ./i18n"
        }
    },
    "devDependencies": {
        "@dhis2/d2-i18n-extract": "^1.0.7",
        "@dhis2/d2-i18n-generate": "^1.0.19",
        "husky": "^1.1.2",
        "postcss-rtl": "^1.3.2",
        "prettier": "^1.14.3"
    }
}
