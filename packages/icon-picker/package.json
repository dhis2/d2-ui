{
    "name": "@dhis2/d2-ui-icon-picker",
    "version": "2.0.0",
    "description": "DHIS2 component for selecting icon",
    "main": "./build/cjs/index.js",
    "module": "./build/es/index.js",
    "license": "BSD-3-Clause",
    "peerDependencies": {
        "d2": "^31.1.1",
        "react": "^16.0.0",
        "react-dom": "^16.0.0"
    },
    "author": "Mark Polak",
    "contributors": [
        "Nicolay Ramm",
        "Kjetil Svalestuen <kjetil@dhis2.org>",
        "Viktor Varland <viktor@dhis2.org>",
        "Edoardo Sabadelli <edoardo@dhis2.org>"
    ],
    "scripts": {
        "prebuild": "rimraf ./build/*",
        "build": "npm run build:commonjs && npm run build:es",
        "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir build/cjs --ignore spec.js",
        "build:es": "cross-env BABEL_ENV=es babel src --out-dir build/es --ignore spec.js",
        "watch": "npm run build:es --  --watch",
        "test-ci": "jest --config=../../jest.config.js packages/icon-picker"
    },
    "dependencies": {
        "@dhis2/d2-ui-core": "^1.4.0",
        "babel-runtime": "^6.26.0",
        "lodash": "^4.17.10",
        "material-ui": "^0.20.0",
        "prop-types": "^15.5.10"
    },
    "jest": {
        "transformIgnorePatterns": [
            "node_modules/(?!react-native|react-navigation)/"
        ]
    },
    "publishConfig": {
        "access": "public"
    }
}
