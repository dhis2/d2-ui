{
  "name": "d2-ui-sharing",
  "version": "1.0.1",
  "description": "DHIS2 component for sharing dialog",
  "main": "./build/cjs/index.js",
  "module": "./build/es/index.js",
  "license": "BSD-3-Clause",
  "private": true,
  "peerDependencies": {
    "d2": "^29.1.1",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "author": "Mark Polak",
  "contributors": [
    "Nicolay Ramm",
    "Kjetil Svalestuen <kjetil@dhis2.org>",
    "Viktor Varland <viktor@dhis2.org>",
    "Edoardo Sabadelli <edoardo@dhis2.org>"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-imports": "^1.5.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "fs-extra": "^5.0.0",
    "jest": "^22.4.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "webpack": "3",
    "webpack-dev-server": "^2.11.1"
  },
  "scripts": {
    "prebuild": "rimraf ./build",
    "copyfiles": "node ./scripts/copy-files.js",
    "build": "npm run copyfiles && npm run build:commonjs && npm run build:es && npm run build:umd",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir build/cjs --ignore spec.js",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir build/es --ignore spec.js",
    "build:umd": "cross-env && webpack -p",
    "watch": "npm run build:es --  --watch"
  },
  "dependencies": {
    "d2-ui": "^29.0.5",
    "material-ui": "^0.19.3",
    "prop-types": "^15.5.10",
    "recompose": "^0.26.0",
    "rxjs": "^5.5.6"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ]
  }
}
