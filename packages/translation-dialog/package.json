{
  "name": "@dhis2/d2-ui-translation-dialog",
  "version": "3.0.0",
  "description": "Translation Dialog for DHIS2",
  "main": "./build/cjs/index.js",
  "module": "./build/es/index.js",
  "license": "BSD-3-Clause",
  "peerDependencies": {
    "d2": "^31.1.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "author": "Mark Polak",
  "contributors": [
    "Nicolay Ramm",
    "Kjetil Svalestuen <kjetil@dhis2.org>",
    "Viktor Varland <viktor@dhis2.org>",
    "Edoardo Sabadelli <edoardo@dhis2.org>"
  ],
  "scripts": {
    "prebuild": "rimraf ./build/*",
    "build": "npm run build:commonjs && npm run build:es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir build/cjs --ignore spec.js",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir build/es --ignore spec.js",
    "watch": "npm run build:es --  --watch",
    "test-ci": "jest --config=../../jest.config.js packages/translation-dialog"
  },
  "dependencies": {
    "@dhis2/d2-ui-core": "^1.4.0",
    "@material-ui/core": "^3.3.1",
    "@material-ui/icons": "^3.0.1",
    "babel-runtime": "^6.26.0",
    "d2-utilizr": "^0.2.15",
    "prop-types": "^15.5.10",
    "react-select": "^2.0.0",
    "recompose": "^0.26.0",
    "rxjs": "^5.5.7"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
