{
    "presets": [["env", { "modules": false }], "react", "stage-2"],
    "plugins": [
        "transform-runtime"
    ],
    "env": {
        "commonjs": {
            "presets": [
                [
                    "env",
                    {
                        "modules": "commonjs"
                    }
                ],
                "react",
                "stage-2"
            ],
            "plugins": [
                "transform-class-properties",
                "transform-runtime"
            ]
        },
        "es": {
            "presets": [
                [
                    "env",
                    {
                        "modules": false
                    }
                ],
                "react",
                "stage-2"
            ],
            "plugins": [
                "transform-runtime"
            ]
        },
        "test": {
            "presets": ["env", "react", "stage-2"],
            "plugins": [
                "transform-es2015-modules-commonjs",
                "transform-runtime"
            ]
        }
    },
}

