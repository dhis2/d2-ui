{
  "name": "d2-ui",
  "version": "28.0.0-1",
  "description": "d2-ui",
  "main": "index.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "examples": "npm run start",
    "coverage": "jest --coverage",
    "start": "webpack-dev-server",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "./node_modules/.bin/eslint src",
    "prebuild": "npm run validate && npm test",
    "preversion": "npm run validate && npm test",
    "build": "npm run build-only",
    "build-only": "rm -rf lib/ && babel src --out-dir lib && compass compile && npm run build-stand-alone-header-bar",
    "prebuild-stand-alone-header-bar": "rm -rf dist &>/dev/null || true",
    "build-stand-alone-header-bar": "webpack -p && cp -r ./examples/external-header-bar/index.html dist",
    "validate": "yarn check || npm ls --depth 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhis2/d2-ui.git"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.16.0",
    "classnames": "^2.2.5",
    "codeclimate-test-reporter": "^0.4.1",
    "colors": "^1.1.2",
    "css-loader": "^0.26.2",
    "d2": "^27.0.0",
    "d2-utilizr": "^0.2.11",
    "d3-format": "^1.1.0",
    "d3-scale": "^1.0.3",
    "enzyme": "^2.9.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-config-dhis2": "^3.0.2",
    "jest": "^21.1.0",
    "jest-enzyme": "^3.8.2",
    "lodash": "^4.17.4",
    "loglevel": "^1.4.1",
    "material-ui": "0.17",
    "node-sass": "^4.5.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.3.2",
    "react-color": "^2.11.3",
    "react-dom": "^15.6.1",
    "react-stub-context": "^0.7.0",
    "react-tap-event-plugin": "^2.0.1",
    "rxjs": "^5.4.3",
    "sass-loader": "^6.0.2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-visualizer-plugin": "^0.1.5"
  },
  "pre-commit": [
    "validate",
    "test"
  ]
}
