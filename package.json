{
  "name": "d2-ui",
  "version": "24.1.12",
  "description": "d2-ui",
  "main": "index.js",
  "scripts": {
    "examples": "npm run start",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --report text --report html --report lcov node_modules/mocha/bin/_mocha -- --reporter dot --compilers js:babel-core/register --require config/setup.js --recursive test",
    "start": "webpack-dev-server",
    "test": "mocha --reporter spec --compilers js:babel-core/register --require config/setup.js --recursive test",
    "test-watch": "mocha --reporter dot --compilers js:babel-core/register --require config/setup.js --recursive test --watch",
    "lint": "eslint src && eslint --env node,mocha --global expect,sinon --rule 'no-unused-expressions: 0' test && scss-lint",
    "prebuild": "npm run validate && npm test",
    "preversion": "npm run validate && npm test",
    "build": "npm run build-only",
    "build-only": "babel src --out-dir lib && compass compile",
    "build-stand-alone-header-bar": "webpack -p && cp ./examples/external-header-bar/index.html build",
    "validate": "npm ls --depth 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhis2/d2-ui.git"
  },
  "license": "BSD-3-Clause",
  "peerDependencies": {
    "material-ui": "0.14.4"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "3.5.0",
    "classnames": "^2.2.3",
    "coveralls": "^2.11.8",
    "css-loader": "^0.23.1",
    "d2": "^24.0.2",
    "enzyme": "1.5.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-config-dhis2": "^2.0.2",
    "eslint-plugin-react": "^4.2.3",
    "fbjs": "^0.8.2",
    "http-proxy": "git+https://github.com/nicolayr/node-http-proxy.git",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "istanbul": "^0.4.2",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jquery": "^2.2.3",
    "jsdom": "^8.1.0",
    "lodash.isfinite": "^3.3.0",
    "lodash.isfunction": "^3.0.8",
    "loglevel": "1.4.0",
    "material-ui": "^0.14.4",
    "mocha": "2.4.5",
    "mocha-jsdom": "^1.1.0",
    "mocha-silent-reporter": "^1.0.0",
    "moment": "^2.11.2",
    "node-sass": "^3.4.2",
    "precommit-hook": "^3.0.0",
    "react": "^0.14.8",
    "react-addons-test-utils": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-stub-context": "^0.4.0",
    "react-tap-event-plugin": "^0.2.0",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "d2-utilizr": "^0.2.4",
    "d3-color": "^0.4.2",
    "d3-format": "^0.6.0",
    "d3-scale": "^0.7.2",
    "draft-js": "^0.7.0",
    "lodash": "^4.12.0",
    "loglevel": "1.4.0",
    "moment": "^2.11.2",
    "react-color": "^1.3.8",
    "rx": "^4.0.8"
  },
  "pre-commit": [
    "validate",
    "test"
  ]
}
